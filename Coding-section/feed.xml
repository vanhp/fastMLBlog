---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <link href="{{- site.url -}}{{- site.baseurl -}}/Coding-section/feed.xml" rel="self" type="application/atom+xml"/>
    <link href="{{- site.url -}}{{- site.baseurl -}}/Coding-section/" rel="alternate" type="text/xml"/>
    <updated>{{- site.time | date_to_xmlschema -}}</updated>
    <id>{{- site.url -}}{{- site.baseurl -}}/ML-section/feed.xml </id>
    <title>{{- site.title -}} /Blog </title>
    <subtitle>{{- site.description | strip_html | lstrip | rstrip -}}</subtitle>
    <author>
        <name>
            {%- if site.author -%}
                {{- site.author -}}
            {%- else -%}
                {{- site.title -}}
            {%- endif -%}
        </name>
    </author> 
    {%- for post in site.posts limit: 10 -%}
        {%- if post.categories=="Coding" -%}
            <entry> 
                <title>{{- post.title -}}</title> 
                <id>{{- site.url -}}{{- site.baseurl -}}{{- post.url -}} </id>
                <link href="{{- site.url -}}{{- site.baseurl -}}{{- post.url -}}" />
                <updated>{{- post.date | date_to_xmlschema -}}</updated>
                <summary> {%- if post.excerpt -%}
                             {{- post.excerpt | strip_html | lstrip | rstrip -}}
                            {%- endif -%}
                </summary>
            </entry>
        {%- endif -%}
    {%- endfor -%}
</feed>